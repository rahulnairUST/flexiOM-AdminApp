<core:FragmentDefinition xmlns="sap.ui.table" xmlns:m="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
<m:VBox class="sapUiSmallMargin">
	<f:SimpleForm id="idDisplayRowSettingsForm" editable="true" layout="ResponsiveGridLayout" title="{i18n>rowSettings}" >
		<f:toolbar>
		<m:Toolbar>
			<m:Title text="{i18n>rowSettings}"/>
			<m:ToolbarSpacer />
			<m:Button icon="sap-icon://edit" text="{i18n>editBtn}" tooltip="{i18n>editBtn}" press="onEditRowSetPressed"/>
		</m:Toolbar>
		</f:toolbar>
		<f:content>
        <Table id="idDispRowSettingsTable" rows="{rowData>/results}" selectionMode="None" visibleRowCount="7">
        <columns>
			<Column width="15rem">
				<m:Label text="{i18n>configLineName}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>LineName}" />
				</template>
			</Column>
            <Column width="15rem">
				<m:Label text="{i18n>rowDesc}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>Description}" />
				</template>
			</Column>
            <Column width="5rem">
				<m:Label text="{i18n>sortPos}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>Sorting}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>grpLevel}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>CvcGroupIdx}" />
				</template>
			</Column>
            <Column width="5rem">
				<m:Label text="{i18n>editable}" wrapping="true"/>
				<template>
					<m:CheckBox editable="false" selected="{rowData>Editable}"/>
				</template>
			</Column>
            <Column width="5rem">
				<m:Label text="{i18n>useDraft}" wrapping="true"/>
				<template>
					<m:CheckBox editable="false" selected="{rowData>/Draft}"/>
				</template>
			</Column>
            <Column width="5rem">
				<m:Label text="{i18n>keyFlagSelc}" wrapping="true"/>
				<template>
					<m:CheckBox editable="false" selected="{rowData>AllowKeyfgSelection}"/>
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>tsTable}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>TimeseriesTable}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>selectionIDRead}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>ReadSelectionID}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>selectionIDWrite}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>SaveSelectionID}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>configLineNameSave}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>SaveSelectionID}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>configLineNameBackup}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>LineNameBackup}" />
				</template>
			</Column>
            <Column width="5rem">
				<m:Label text="{i18n>cnChkSave}" wrapping="true"/>
				<template>
					<m:CheckBox editable="false" selected="{rowData>SaveConsCheck}"/>
				</template>
			</Column>
            <Column width="5rem">
				<m:Label text="{i18n>deltaUpload}" wrapping="true"/>
				<template>
					<m:CheckBox editable="false" selected="{rowData>DeltaUpdate}"/>
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>calculationID}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>CalculationID}" />
				</template>
			</Column>
            <Column width="6rem">
				<m:Label text="{i18n>calcSequence}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>CalculationSequence}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>cvcFormatFOcurrance}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>CvcFormat1stOccurrance}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>cvcDisplay}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>CvcDisplay}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>cvcColor}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>CvcColor}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>cvcColorCode}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>CvcColorCode}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>kfColorCode}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>KeyfigureColorCode}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label text="{i18n>buColorCode}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>BucketColorCode}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label wrapping="true" text="{i18n>cvcStDisplay}" />
				<template>
					<m:Text text="{rowData>CvcStatusDisplay}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label wrapping="true" text="{i18n>cvcColorCode}" />
				<template>
					<m:Text text="{rowData>CvcStatusColorCode}" />
				</template>
			</Column>
            <Column width="10rem">
				<m:Label wrapping="true" text="{i18n>addRemSO}" />
				<template>
					<m:Text text="{rowData>AddRemoveOrders}" />
				</template>
			</Column>
            <Column width="5rem">
				<m:Label text="{i18n>notesID}" wrapping="true"/>
				<template>
					<m:Text text="{rowData>NotesTextID}" />
				</template>
			</Column>
        </columns>
        </Table>
		</f:content>
	</f:SimpleForm>
</m:VBox>
</core:FragmentDefinition>